{
  "name": "react-query-builder",
  "version": "1.0.0",
  "description": "A slightly opinionated query builder component built for React",
  "repository": "https://github.com/bpk68/react-query-builder",
  "keywords": [
    "react",
    "querybuilder",
    "query",
    "builder",
    "operators",
    "ui",
    "component",
    "expression"
  ],
  "main": "dist/index.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "verbose": true,
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "author": "Rob Kendal c/o IAM Cloud Solutions Ltd",
  "license": "ISC",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.3",
    "@babel/core": "7.8.3",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.8.3",
    "@babel/preset-react": "7.8.3",
    "@testing-library/jest-dom": "5.0.2",
    "@testing-library/react": "9.5.0",
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "5.1.1",
    "css-loader": "3.4.2",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.1",
    "eslint": "6.8.0",
    "eslint-config-react-app": "5.0.2",
    "eslint-plugin-flowtype": "4.6.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "file-loader": "5.0.2",
    "html-webpack-plugin": "3.2.0",
    "image-webpack-loader": "6.0.0",
    "jest": "24.9.0",
    "jest-enzyme": "7.1.2",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "react-id-generator": "3.0.0",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-test-renderer": "16.12.0",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.1",
    "webpack-cli": "3.3.10",
    "webpack-merge": "4.2.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.13.0",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "nanoid": "^2.1.11",
    "prop-types": "^15.7.2",
    "react-datepicker": "^2.14.1"
  },
  "scripts": {
    "pre": "npm run lint && npm run test && npm run build",
    "test": "jest",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "build": "webpack --config config/webpack.prod.js",
    "build:dev": "webpack --config config/webpack.dev.js",
    "start": "webpack-dev-server --open --config config/webpack.dev.js",
    "release": "npm run pre && npm publish --access=public"
  }
}
